{
  "name": "seo-audit-backend",
  "version": "1.0.0",
  "description": "Backend for SEO Audit Tool",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset --force",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed": "ts-node scripts/seed-mock-data.ts",
    "seed:enhanced": "ts-node scripts/seed-enhanced.ts",
    "db:migrate-and-seed": "npm run migrate:deploy && npm run seed:enhanced",
    "db:reset-and-seed": "npm run migrate:reset && npm run seed:enhanced",
    "db:health": "ts-node -e \"import('./src/config/database').then(async ({databaseManager}) => { try { await databaseManager.connect(); console.log('✅ Database healthy'); await databaseManager.disconnect(); } catch(e) { console.error('❌ Database unhealthy:', e); process.exit(1); } })\"",
    "db:optimize": "ts-node scripts/optimize-database.ts",
    "db:monitor": "ts-node scripts/monitor-database-health.ts",
    "db:setup": "npm run migrate:deploy && npm run db:optimize && npm run seed:enhanced",
    "db:full-check": "npm run db:health && npm run db:monitor",
    "db:test": "ts-node scripts/test-database-integration.ts",
    "db:validate": "npm run db:test && npm run db:monitor",
    "reset-db": "prisma migrate reset --force && npm run seed",
    "health-check": "ts-node src/scripts/analysis-system-health-check.ts",
    "db:migrate": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio"
  },

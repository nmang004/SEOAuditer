# Railway Production Environment Variables
# These will be automatically set by Railway or need to be configured in Railway dashboard

# Auto-provided by Railway
DATABASE_URL=postgresql://postgres:password@hostname:5432/railway
DATABASE_PROXY_URL=postgresql://postgres:password@proxy.railway.internal:5432/railway
REDIS_URL=redis://default:password@hostname:6379
PORT=4000
RAILWAY_ENVIRONMENT=production

# Must be set manually in Railway dashboard
NODE_ENV=production
JWT_SECRET=your-secure-jwt-secret-at-least-32-characters-long
ALLOWED_ORIGINS=https://seoauditer.netlify.app,https://seodirector.com
LOG_LEVEL=info

# Optional - Email service
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Optional - Monitoring
SENTRY_DSN=
GOOGLE_ANALYTICS_ID=